}

.post-page-infobox{
  bottom: 0;
  right: 0;
  color: #9e9e9e;
  background-color: #eeeeee;
  padding: 10px;
  border-right: 3px solid #0288d1;
}

.post-page-infobox p{
  margin: 0 auto;
}

.postpage-card-content{
  color: #757575;
  border-right: 3px solid #0288d1;
  padding-right: 20px;
}

.postpage-card-content p{
    padding-top: 30px;
}

.postpage-ad{
  text-align: center;
  margin: 0 auto;
}

#containingDiv * {
  width: 100% !important;
  height: auto !important;
}

#containingDiv>p>img {
    width: 100% !important;
    height: auto !important;
}

#containingDiv>p>iframe {
    width: 100% !important;
    height: 20vw !important;
    min-height: 20px !important;
    float: right;
}

/*---- Post page end ----*/



/*---- Contact page ----*/
.contact textarea {
    width: 100%;
    height: 150px;
    padding: 12px 20px;
    box-sizing: border-box;
    border: 2px solid #ccc;
    border-radius: 4px;
    font-size: 16px;
    resize: none;
}
