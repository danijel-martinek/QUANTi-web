/*---- Home page ----*/
.top-margin-10{
  margin-top: 100px;
  margin-left: 0px;
}

.parallax_overlay {
    height: auto;
}
.parallax_overlay img {
    width:100%;
    vertical-align:top;
    object-fit: cover;
}
.parallax_overlay:after {
    content:'\A';
    position:absolute;
    width:100%; height:100%;
    background:rgba(0,0,0,0.7);
    opacity:1;
}

[id^=section]{

     font-size: 1em;

            line-height: 150%;

            padding: 1em;

            text-align: justify;

            text-justify: distribute;

        }

        .p-font-image{

            font-size: 5em;

            margin-top: 0.5em;

            margin-bottom: 0.5em;  

        }



        .p-font-image i{

            padding: 40px;

            border: 5px solid transparent;

            -moz-border-image: -moz-linear-gradient(top, #3acfd5 0%, #3a4ed5 100%);

            -webkit-border-image: -webkit-linear-gradient(top, #3acfd5 0%, #3a4ed5 100%);

            border-image: linear-gradient(to bottom, #3acfd5 0%, #3a4ed5 100%);

            border-image-slice: 1;





            background: rgba(255,94,236,1);

            background: -moz-linear-gradient(-45deg, rgba(255,94,236,1) 0%, rgba(94,140,255,1) 38%, rgba(94,172,255,1) 66%, rgba(7,235,197,1) 100%);

            background: -webkit-gradient(left top, right bottom, color-stop(0%, rgba(255,94,236,1)), color-stop(38%, rgba(94,140,255,1)), color-stop(66%, rgba(94,172,255,1)), color-stop(100%, rgba(7,235,197,1)));

            background: -webkit-linear-gradient(-45deg, rgba(255,94,236,1) 0%, rgba(94,140,255,1) 38%, rgba(94,172,255,1) 66%, rgba(7,235,197,1) 100%);

            background: -o-linear-gradient(-45deg, rgba(255,94,236,1) 0%, rgba(94,140,255,1) 38%, rgba(94,172,255,1) 66%, rgba(7,235,197,1) 100%);

            background: -ms-linear-gradient(-45deg, rgba(255,94,236,1) 0%, rgba(94,140,255,1) 38%, rgba(94,172,255,1) 66%, rgba(7,235,197,1) 100%);

            background: linear-gradient(135deg, rgba(255,94,236,1) 0%, rgba(94,140,255,1) 38%, rgba(94,172,255,1) 66%, rgba(7,235,197,1) 100%);

            -webkit-background-clip: text;

            -webkit-text-fill-color: transparent;

        }



        .p-partners{

            border: 5px solid transparent;

            -moz-border-image: -moz-linear-gradient(top, #3acfd5 0%, #3a4ed5 100%);

            -webkit-border-image: -webkit-linear-gradient(top, #3acfd5 0%, #3a4ed5 100%);

            border-image: linear-gradient(to bottom, #3acfd5 0%, #3a4ed5 100%);

            border-image-slice: 1;

        }

    

        @media only screen and (min-width: 599px) {

            [id^=section]{

                font-size: 1.5em;

            }

            .p-text p{

                margin-left: 3em;

            }



            .p-text h3{

                margin-left: 1em;

            }



            .p-partners{

                margin-bottom: -2em;

            }

        }



        .without-p-img img{

            width: 150vw !important;

            height: auto !important;

            margin-left: -25vw !important;

        }

/*---- Home page end ----*/



/*---- News page ----*/
.caption-news h4{
  display:inline-flex;
  background-color:rgba(0,0,0,0.7);
  padding: 10px;
  font-size: 180%;
}
.caption-news h6{
  display:inline-flex;
  background-color:rgba(0,0,0,0.7);
  padding: 10px;
  font-size: 150%;
}
.caption-link-news{
  display:inline;
  background-color:rgba(0,0,0,0.7);
  font-size: 1em;
  text-transform: uppercase;
  padding: 5px;
  color: #fff;
}

.card-image-news-container{
  position: fixed;
  min-width: 100%;
  min-height: 100%;
}

.card-image-news{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  min-width: 100%;
  min-height: 100%;
  overflow-x: hidden;
}

.card-title-color{
  color: #424242;
}

.card-content{
  margin-top: -20px;
}

.card-date-news{
  text-align: center;
  border: 0.1em solid #a9a9a9;
  border-right: none;
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
  margin-top: 10px;
}
.divider-margin{
  margin-top: 10px;
  margin-bottom: 20px;
}
.card-title{
  margin-bottom: 30px !important;
}

.featured-post-title{
  color: #ffffff !important;
}
/*---- News page end ----*/



/*---- Events page ----*/
#countdown{
  margin: 0 auto;
  font-size: 2em;
}

.events-main-card-title{
  margin-top: 10px;
}

/*---- Events page end ----*/



/*---- Post page ----*/
.postpage-bg-card{
  margin-top: 60px;
}

.postpage-main-image{
  width: 100%;
  height: auto;
}

.postpage-main-img-bg{
  margin-top: -50px;
}

.postpage-title{
  background-color: rgba(0,0,0,0.5);
  margin-left: 11.5px;
  margin-bottom: 10px !important;
  font-size: 1.2em !important;
